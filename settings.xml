<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <mirrors>
        <mirror>
            <id>central</id>
            <mirrorOf>central</mirrorOf>
            <url>https://repo.maven.apache.org/maven2</url>
        </mirror>
        <mirror>
            <id>maven-central</id>
            <mirrorOf>central</mirrorOf>
            <url>https://repo1.maven.org/maven2/</url>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>default</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>https://repo.maven.apache.org/maven2</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>default</activeProfile>
    </activeProfiles>
    <servers>
        <server>
            <id>central</id>
            <configuration>
                <httpConfiguration>
                    <get>
                        <params>
                            <param>
                                <name>http.socket.timeout</name>
                                <value>30000</value>
                            </param>
                            <param>
                                <name>http.connection.timeout</name>
                                <value>30000</value>
                            </param>
                            <param>
                                <name>http.connection-manager.timeout</name>
                                <value>30000</value>
                            </param>
                            <param>
                                <name>http.protocol.head-body-timeout</name>
                                <value>30000</value>
                            </param>
                            <param>
                                <name>http.connection.stalecheck</name>
                                <value>true</value>
                            </param>
                        </params>
                    </get>
                </httpConfiguration>
            </configuration>
        </server>
    </servers>
</settings>
